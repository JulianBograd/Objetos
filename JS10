// Copiar dos objetos

const producto = {
    nombre: 'Monitor de 20 pulgadas',
    precio: 300,
    disponible: true,
}
console.log(producto);

const características = {
    peso: '1kg',
    medida: '1m',
}
console.log(características);

const resultado = Object.assign(producto, características); //Une los dos objetos
console.log(resultado);

// Spread Operator o Rest Operator
const resultado2= { ...producto, ...características}; // Los 3puntos significa "copiar variable" y la asigna al objeto
console.log(resultado2);
