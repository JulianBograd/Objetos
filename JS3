// Agregar o eliminar propiedades de un objeto

const producto = {
    nombre: 'Monitor de 20 pulgadas',
    precio: 300,
    disponible: true,
}

console.log(producto);

// Agregar propiedades
producto.imagen = 'imagen.jpg';
console.log(producto);

// Eliminar propiedades
delete producto.disponible;


console.log(producto);
